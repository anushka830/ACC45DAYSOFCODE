//Printing  Duplicate elements in the array 
#include <stdio.h>


int main()
{

   int arr[50],num,i,j;

   printf("Enter the number of elements of the array \n");

   scanf("%d",&num);
   
   printf("Enter the elements of the array \n");

   for(i=0; i<num; i++){

       scanf("%d",&arr[i]);

   }
   printf("Duplicate elements in the array are\n");

   for(i=0; i<num; i++){

       for(j=i+1;j<num; j++){

           if(arr[i]==arr[j]){

               printf("%d  ",arr[j]);
           }
       }
   }
    return 0;
}
