#include <stdio.h>

int factorial(int number ){

    if(number==0 || number ==1){

        return 1;

    }

    else{

        return number*factorial(number-1);
    }

}


int main()

{
    int number,c;

    printf("Enter the number \n");

    scanf("%d",&number);
   
    printf("%d",factorial(number));
    

    
    return 0;
}
