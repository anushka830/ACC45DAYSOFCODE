#include<stdio.h>

int power(int,int);

int main(){

int n,i;

printf("Enter a number: ");

scanf("%d",&n);

printf("Enter a power: ");

scanf("%d",&i);

printf("%d",power(n,i));

return 0;

}

int power(int i,int j){

    if(j==0)

    return 1;

    else{

        return (i*power(i,j-1));

    }

}
