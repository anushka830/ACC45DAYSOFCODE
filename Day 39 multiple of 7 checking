#include<stdio.h>

void main() {

    int i,count=0,size,el;

    printf("Enter array size: ");

    scanf("%d", &size);

    printf("Elements to check: ");

    scanf("%d", &el);

    int a[size];

    for(i=0;i<size;i++){

        printf("Enter for a[%d]: ",i);

        scanf("%d", &a[i]);

        if(a[i]%7==0){

            count++;

        }

    }

    if(el<=count) printf("YES");

    else printf("NO");
}
