#include<stdio.h>

int main(){

int a[100],b[100],n,i,num=0;

printf("Enter the size of array: ");

scanf("%d",&n);

printf("Enter first array element:");

for(i=0;i<n;i++){

	scanf("%d",&a[i]);

}

printf("Enter second array element:");

for(i=0;i<n;i++){

	scanf("%d",&b[i]);

}
for(i=0;i<n;i++){

	if(a[i]==b[i]){

		num++;
	}
}
if(num==n){

    printf("Yes");

}

	else

	{
		printf("N0");
		
	}

return 0;
}
