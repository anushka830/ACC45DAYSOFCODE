#include<stdio.h>

int main(){

int arr[50],num,i,large,fact=1;

printf("Enter the number of element in the array \n");

scanf("%d",&num);

printf("Please enter the elements \n");

for(i=0; i<num;i++){

   scanf("%d",&arr[i]);

   large=arr[0];

}

for(i=0; i<num; i++){

   if(arr[i]>large){

      large=arr[i];

   }

}

for(i=1; i<=large; i++){

   fact=fact*i;

}

printf("%d",fact);

return 0;

}
