#include<stdio.h>

Void main()

{int a;

printf("Enter the week number \n");

scanf("%d", &a);

switch (a)  

{

case 1:

    printf("HELLO today is sunday ");
    break;

case 2:

    printf("Hello today is Monday");
    break;

case 3:

    printf("Hello today is Tuesday");
    break;

case 4:

    printf("Hello today is Wednesday");
    break;

case 5:

    printf("Hello today is Thrusday");
    break;

case 6:

    printf("Hello today is Friday");
    break;

case 7:

    printf("Hello today is Saturday");
    break;


default:

    break;
}
}
