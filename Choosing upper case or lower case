#include<stdio.h>

Void main()

char a ;

printf("Enter the character \n");

scanf("%c", &a);

if(a>='a' && a<='z'){

    printf("%c is a lowercase alphabet  ", a);

}

else if (a>='A' && a<='Z'){

    printf("%c is a uppercase alphabet ", a);

}

else {

    printf("%c  is not  an alphabet so please enter only alphabet ", a);

}
